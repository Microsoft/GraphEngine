<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.9083.0</Version>
    <FileVersion>1.0.9083.0</FileVersion>
    <AssemblyVersion>1.0.9083.0</AssemblyVersion>
    <PackageVersion>1.0.9083</PackageVersion>
    <Authors>Microsoft Graph Engine Team</Authors>
    <Company>Microsoft Corporation</Company>
    <Description>Provides GraphEngine-specific Service Fabric runtime support.</Description>
    <Copyright>Copyright 2018</Copyright>
    <Title>GraphEngine Service Fabric Infrastructure Runtime</Title>
    <PackageTags>Graph Engine Trinity Service Fabric Dynamic Cluster</PackageTags>
    <PackageLicenseUrl>https://github.com/Microsoft/GraphEngine/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://www.graphengine.io</PackageProjectUrl>
    <PackageIconUrl>https://www.graphengine.io/img/logo-64x64.png</PackageIconUrl>
    <PackageId>GraphEngine.ServiceFabric.Infrastructure</PackageId>
    <OutputPath>..\..\..\..\bin</OutputPath>
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GraphEngine.Core" Version="1.0.9083" />
    <PackageReference Include="GraphEngine.DynamicCluster" Version="1.0.9083" />
    <PackageReference Include="Microsoft.ServiceFabric" version="6.0.232" />
    <PackageReference Include="Microsoft.ServiceFabric.Data" version="2.8.232" />
    <PackageReference Include="Microsoft.ServiceFabric.Diagnostics.Internal" version="2.8.232" />
    <PackageReference Include="Microsoft.ServiceFabric.Services" version="2.8.232" />
    <PackageReference Include="Nito.AsyncEx.Tasks" version="1.1.0" />
    <PackageReference Include="Nito.Disposables" version="1.0.0" />
    <PackageReference Include="System.Collections.Immutable" version="1.4.0" />
    <PackageReference Include="System.IO.Compression.ZipFile" version="4.3.0" />
    <PackageReference Include="System.Runtime.Numerics" version="4.3.0" />
    <PackageReference Include="System.ValueTuple" version="4.4.0" />
  </ItemGroup>
</Project>
