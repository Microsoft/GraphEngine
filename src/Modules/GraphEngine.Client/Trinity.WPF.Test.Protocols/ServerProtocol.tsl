// Testing with RDF Triples


// RDF Vocabularies and Namespace IRIs

enum NamespaceIRIParts
{
	NamespacePrefix,
	NamespaceIRI,
	ResourceName,
}

// model of RDF/XML Normnative syntax
struct RDFTriple
{
	string Subject;
	string Predicate;
	string Object;
	// Metadata 
	string UriPart;
	string Namespace;
	string Prefix;
}

// RDF Statement ...

struct RDFStatement
{
	List<RDFTriple> rdfStatements;
}

struct StatusCodeResponse
{
	string statusCode;
	int    returnCode;
	int    errorCode;
}

struct TripleStream
{
	List<RDFTriple> triples;
}

struct ErrorCodeResponse
{
	int errno;
}

protocol StreamTriplesAsync
{
	Type: Asyn;
	Request: TripleStream;
	Response: ErrorCodeResponse;
}

protocol PostTriplesToServer
{
	Type: Asyn;
	Request: TripleStream;
	Response: ErrorCodeResponse;
}

// API Productions

protocol PushTripleToClientAsync
{
	Type: Asyn;
	Request: RDFStatement;
	Response: StatusCodeResponse;
}

// Declare TSL Module 
module GETestFacilityServer
{
	protocol PushTripleToClientAsync;
	protocol StreamTriplesAsync;
}
