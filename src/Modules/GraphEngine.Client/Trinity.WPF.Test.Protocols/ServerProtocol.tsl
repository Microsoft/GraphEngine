// Testing with RDF Triples


// RDF Vocabularies and Namespace IRIs

enum NamespaceIRIParts
{
	NamespacePrefix,
	NamespaceIRI,
	ResourceName,
}

// model of RDF/XML Normnative syntax
struct RDFTriple
{
	string Subject;
	string Predicate;
	string Object
	// Metadata 
	string UriPart;
	string Namespace;
	string Prefix;
}

// RDF Statement ...

struct RDFStatement
{
	List<RDFTriple> rdfStatement;
}

struct StatusCodeResponse
{
	string statusCode;
	int    returnCode;
	int    errorCode;
}

// API Productions

protocol PushTripleToClientAsync
{
	Type: Async;
	Request RDFStatement;
	Response: StatusCodeResponse
}


